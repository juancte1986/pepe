<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="ar.edu.uces.progweb2.agenda.model.HallMeeting" table="sala_reunion">
		<id column="id" name="id">
			<generator class="native" />
		</id>
		<many-to-one name="hall"
			column="hallId"
			cascade="none"
			not-null="true" 
			class="ar.edu.uces.progweb2.agenda.model.Hall">
		</many-to-one>
	</class>
</hibernate-mapping>